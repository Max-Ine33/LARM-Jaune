  <!--On lance gmapping -->
  <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping"/>
  
  <!--On lance rviz qui a une configuration donnÃ©e -->
  <node pkg="rviz" type="rviz" name="rviz" args=" -d $(find grp-jaune)/src/data/challenge2.rviz"/>

  <param name="/use_sim_time" value="true" />
  
  
</launch>
