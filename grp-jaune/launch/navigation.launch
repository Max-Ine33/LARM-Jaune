<?xml version="1.0"?>
<launch>
  
	<include file="$(find larm)/launch/challenge-3.launch"/>
	<!-- Run the map server lien : https://bitbucket.org/imt-mobisyst/mb6-tbot/src/master/turtlebot2/turtlebot_apps/turtlebot_navigation/launch/includes/move_base.launch.xml -->
	<arg name="map_file" default="$(find turtlebot_navigation_gazebo)/maps/my_map.yaml"/>
	<node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)"/>
	
	<!-- Run AMCL -->
	<include file="$(find turtlebot_navigation)/launch/includes/amcl/amcl.launch.xml"/>
  
	<node pkg="grp-jaune" name="move" type="move_sim.py" output="screen"/>
	<node pkg="grp-jaune" name="listener" type="listener_sim.py" output="screen"/>
	<node pkg="tf" type="static_transform_publisher" name="base_footprint_in_odom" args="2.3 4.0 0.01 0.8 0.0 0.0 /odom /base_footprint 80" />
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find grp-jaune)/src/rviz/test_move.rviz" />


</launch>

