<launch>

<remap from="goal" to="odom"/>
<!--<include file="$(find larm)/launch/challenge-1.launch"/>-->
<node pkg="move-to" type="move-to.py" name="gogogo" output="screen" />
<node pkg="tf" type="static_transform_publisher" name="base_footprint_in_odom"
  args="2.3 4.0 0.01 0.8 0.0 0.0 /odom /base_footprint 80" />
 <node pkg="rviz" type="rviz" name="rviz" args="-d $(find move-to)/rviz/test_move.rviz" />

<param name="/use_sim_time" value="true"/>

</launch>
